<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd">

   <bean id="pieCeptionService" class="org.pieShare.pieTools.pieCeption.service.core.PieCeptionService">
       <property name="connectorService" ref="clusterConnectorService"/>
       <property name="commandParserService" ref="commandParserService"/>
   </bean>

    <bean id="clusterConnectorService" class="org.pieShare.pieTools.pieCeption.service.core.ClusterConnectorService">
    </bean>

    <bean id="commandParserService" class="org.pieShare.pieTools.pieCeption.service.commandParser.Argparse4jService">
    </bean>
    
</beans>